						<div class="label" style="left: 1650dp; width:150dp; margin-top:-18dp">VOICE MOD SEND</div>
					</div>
				</div>
				
				<!-- FX delay section -->
				<combo param="DelayType" id="DelayType" class="jucePos comboxtext2" style="left: 1648dp; top: 133dp; width: 340dp; height: 45dp; line-height: 45dp;">
					<combotext></combotext>
				</combo>
				
				<combo param="DelaySync" id="DelaySync" class="jucePos comboxtext2" style="left: 1648dp; top: 201dp; width: 340dp; height: 45dp; line-height: 45dp;">
