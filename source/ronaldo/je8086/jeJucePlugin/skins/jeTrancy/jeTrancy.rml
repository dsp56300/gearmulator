						<input class="abs sliderV slider_row2" param="Osc1Control2FeedbackAmount" id="Osc1Control2" type="range" orientation="vertical" style="top: 4dp; left: 357dp;"/>
						<div class="label" style="left: 246dp; width:100dp; margin-top:-20dp">HARMO- NICS</div>
						<div class="label" style="left: 335dp; width:100dp; margin-top:-20dp">FDBCK AMT</div>						
					</div>	
					
					<!-- Square wave -->
