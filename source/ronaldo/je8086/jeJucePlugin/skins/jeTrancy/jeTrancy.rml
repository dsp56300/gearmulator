					</combo>
					
					<!-- Sync -->
					<combo param="LFO1Sync" id="LFO1Sync" class="jucePos comboxtext2" style="left: 0dp; top: 84dp; width: 310dp; height: 45dp; line-height: 45dp;">
						<combotext></combotext>
					</combo>
					
					<div class="knobs_row2">
						<knob class="knob" param="Lfo1Rate" id="Lfo1Rate"/>
						<knob class="knob" param="Lfo1Fade" id="Lfo1Fade"/>
					</div>
				</div>
				
				<!-- OSC 1 -->
