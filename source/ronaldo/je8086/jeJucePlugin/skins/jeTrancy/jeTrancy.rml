					<div id="ContainerOsc1Mode" class="jucePos" data-class-juceConditionDisabled="!(Osc1Waveform_value == 6)" data-model="partCurrent" >
						<input class="abs sliderV slider_row2" param="Osc1Control1Shape" id="Osc1Control1" type="range" orientation="vertical" style="top: 4dp; left: 264dp;"/>
						<input class="abs sliderV slider_row2" param="Osc1Control2LFO1Depth" id="Osc1Control2" type="range" orientation="vertical" style="top: 4dp; left: 357dp;"/>
						<div class="label" style="left: 246dp; width:100dp; margin-top:-20dp">SHAPE</div>
						<div class="label" style="left: 335dp; width:100dp; margin-top:-20dp">LFO 1 DEPTH</div>							
					</div>				
					
					
